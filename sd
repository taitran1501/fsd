Config = {
    namescript = "Auto find fruit Hub", -- T√™n Script
    logoscript = "91834612666410", -- ID Logo Script
    tacgia = "BLX Group" -- T√™n T√°c Gi·∫£
}

local Settings = {
  JoinTeam = "Marine"; -- Pirates/Marines
  Translator = true; -- true/false
}
while true do
    wait(5) -- Ki·ªÉm tra m·ªói 5 gi√¢y
    for _, v in pairs(game.Workspace:GetChildren()) do
        if v:IsA("Tool") and string.find(v.Name, "Fruit") then
            print("üî• Tr√°i √°c qu·ª∑ ph√°t hi·ªán: " .. v.Name)
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.Handle.CFrame
            wait(2) -- Ch·ªù ƒë·ªÉ nh·∫∑t
            firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart, v.Handle, 0)
            firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart, v.Handle, 1)
            print("‚úÖ ƒê√£ nh·∫∑t: " .. v.Name)
            
            -- ƒê·ªïi server sau khi nh·∫∑t
            local TeleportService = game:GetService("TeleportService")
            local PlaceId = game.PlaceId
            local Servers = TeleportService:GetTeleportSetting("PreferredServers")
            if Servers then
                for _, server in pairs(Servers) do
                    if server.id ~= game.JobId then
                        print("üîÑ ƒêang chuy·ªÉn server...")
                        TeleportService:TeleportToPlaceInstance(PlaceId, server.id, game.Players.LocalPlayer)
                        break
                    end
                end
            end
        end
    end
end
loadstring(game:HttpGet("https://raw.githubusercontent.com/taitran1501/fsd/refs/heads/main/sd"))()
